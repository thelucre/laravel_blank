{
	"name": "anahkiasen/rocketeer",
	"description": "Rocketeer is a fast and simple deployer for the PHP world",
	"license": "MIT",
	"keywords": [
		"laravel",
		"deployment",
		"ssh"
	],
	"authors": [
		{
			"name": "Maxime Fabre",
			"email": "ehtnam6@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"illuminate/support": "~4.1",
		"illuminate/config": "~4.1",
		"illuminate/console": "~4.1",
		"illuminate/container": "~4.1",
		"illuminate/filesystem": "~4.1",
		"illuminate/events": "~4.1",
		"illuminate/remote": "~4.1",
		"illuminate/log": "~4.1"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"nesbot/carbon": "~1.4",
		"patchwork/utf8": "~1.1.18",
		"herrera-io/box": "~1.5.3",
		"phpseclib/phpseclib": "~0.3.5"
	},
	"suggest": {
		"anahkiasen/rocketeer-campfire": "Campfire plugin to create deployments notifications",
		"anahkiasen/rocketeer-slack": "Slack plugin to create deployments notifications"
	},
	"bin": [
		"bin/rocketeer"
	],
	"autoload": {
		"psr-4": {
			"Rocketeer\\": ["src/Rocketeer", "tests"]
		}
	},
	"extra": {
		"branch-alias": {
			"dev-develop": "1.3-dev"
		}
	}
}
